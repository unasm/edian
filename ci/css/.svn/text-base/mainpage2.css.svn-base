.block{
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
}
.shadow{
	-moz-box-shadow:0px 0px 5px #ADACAC;
	-webkit-box-shadow:0px 0px 5px #ADACAC;
	box-shadow:1px 1px 5px #ADACAC;
}
.contSpace{
	margin:0px;
	padding:0px;
}
.dirClick{
