.block{
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
}
.contSpace{
	margin:0px;
	padding:0px;
}
li{
	list-style:none outside none;
}
.clearfix:after{
	visibility:hidden;
	display:block;
	height:0px;
	content:"";
	clear:both;
}
.clearfix{
	display:inline-block;
	display:block;
}
*html .clearfix {
	height:1%;
}
body {
	background:url("../edian/bgdianimg2.png");
	margin:0px auto;
	color:#444;
}
#header{
	height:150px;
	position:relative;
	width:960px;
	margin:0px auto;
	position:relative;
}
#header span{
	background:url("../edian/logo.png") no-repeat;
	position:absolute;
	bottom:0px;
	width:104px;
	height:95px;
}
#topline{
	background:url("../edian/topLineCoose1.png")no-repeat;
	z-index:5;
	height:8px;
}
#denglu{
	position:absolute;
	width:160px;
	height:73px;
	text-align:center;
	right:0px;
	bottom:0px;
	background:url("../edian/mylogo2.png")no-repeat;
}

#denglu img{
	width:200px;
	height:120px;
}
#denglu img{
	-webkit-box-shadow:2px 1px 4px #5B5B5B;
	-moz-box-shadow:2px 1px 4px #5B5B5B;
	box-shadow:2px 1px 4px #5B5B5B;
}
#denglu input:hover{
	background: url("../edian/button.png") rgba(41,37,206,0.2);
}
#denglu .butDenglu{
	height:50px;
	width:81px;
	border:0px;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
	background:url("../edian/button.png");
	margin:5px;
}
#newBody{
	width:960px;
	margin:0px auto;
	background:url("../edian/bodyline22.png");
	margin-top:2px;
}
#dir{
	float:left;
	background:url("../edian/dir-bg.png");
	height:400px;
	width:194px;
	padding:0px;
	border-left:1px solid #e0e0e0;
	margin:-7px 0px 0px 0px;
	overflow:hidden;
}
#dir ul{
	float:left;
	padding:0px;
	margin:0px;
}

#dir .dirmenu{
	width:119px;
	margin:0px;
	font-size:18px;
	height:15px;
	padding:20px 15px 20px 60px;
	border-top:1px solid #ffffff;
	border-bottom:1px solid #cccccc;
}

#dir ul li:hover{
	background:rgba(255,255,255,0.6);
}

#content{
	float:left;
	margin-left:8px;
	width:755px;
	/*
	 * 留下的宽度为763px，留下8个像素的缓冲
	 */
	overflow:hidden;
}
#content .team{
	height:210px;
}
#content .contLi{
	float:left;
	margin:3px 8px 0px 3px;
	width:365px;
	height:90px;
	padding:0px;
	position:relative;
	overflow:hidden;
}
#content li:hover{
	background:rgb(106,106,255);
}
#content .pageDir{
	color:#B8B8B8;
	font-size:26px;
	margin:-11px 0px 0px 13px;
}
#content li img{
	width:64px;
	margin:14px 0px 7px 13px;
	height:64px;
	background:blue;
}
#content .contArt{
	width:278px;
	position:absolute;
	bottom:3px;
	right:0px;
}
#content .contArt .conTitle{
	width:283px;
	height:36px;
	margin-bottom:8px;
	text-overflow:ellipsis;
	padding:0px;
	overflow:hidden;
	color:#969694;
	display:block;
}
#content .contArt .atten{
	font-size:10px;
	margin-bottom:3px;
	color:#B4B4B4;
	letter-spacing:1px;
}
.oneAtten{
	text-overflow:ellipsis;
	white-space:nowrap;
	overflow:hidden;
}
#content .contArt .atten p{
	display:block;
	width:144px;
	height:15px;
	margin:0px;
	padding:0px;
}
#content .contArt .atten .author{
	width:88px;
	margin:0px;
	padding:1px;
	display:block;
}
#content .contArt .atten .time{
	position:absolute;
	bottom:6px;
	right:8px;
}
#bottomDir{
	background:url("../edian/bottom.png");
	width:100%;
	float:left;
	height:65px;
	position:fixed;
	bottom:0px;
}
#bottomDir .botDirli{
	width:20px;
	height:20px;
	float:left;
	padding:6px;
	text-align:center;
	border:1px solid #AAA;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
	margin:18px 5px;
	font-size:18px;
}
#bottomDir li:hover{
	-moz-box-shadow:0px 0px 6px #4DFFFF;
	-webkit-box-shadow:0px 0px 6px #4DFFFF;
	box-shadow:0px 0px 6px #4DFFFF;
	border:1px solid #ececec;
}
#bottomDir ul{
	width:600px;
	margin:0px auto;
	margin-bottom:2px;
}
